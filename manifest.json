{
  "manifest_version": 2,
  "name": "simple redirect",
  "description": "redirect javascript",
  "version": "1.1.0",
  "browser_action": {
    "default_icon": "icon2.png",
    "default_title": "redirect javascript ",
    "default_popup" : "popup.html"
  },
  "background": {
  "scripts": ["background.js"],
   "persistent": true
  },
  "permissions": [
    "tabs"
  ]
}
